{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand --detectOpenHandles",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:fb-api": "npm t -- -c ./jest.integration.config.js ./tests/external/facebook-api.test.ts",
    "prepare": "husky install",
    "style:check": "prettier --check src/**/*.ts",
    "style:stable": "prettier --write src/**/*.ts",
    "style:tests": "prettier --write tests/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.3",
    "mermaid": "^10.3.1",
    "pg-mem": "^1.8.6",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "0.2.29",
    "uuid": "^9.0.0"
  }
}
